{
    "name": "vojtys/bootstrap-datepicker",
    "description": "Bootstrap DatePicker (eternicode/bootstrap-datepicker) for Nette - Forms",
    "type": "library",
    "license": ["BSD-3-Clause"],
    "homepage": "https://github.com/vojtys/bootstrap-datepicker",
    "authors": [
        {
            "name": "Vojtěch Sedláček"
        }
    ],
    "suggest": {
        "ext-openssl": "To make requests via https"
    },
    "require": {
        "php": ">= 5.4",
        "nette/di": ">=2.3.0",
        "nette/forms": ">=2.3.0",
        "nette/utils": ">=2.3.0",
        "eternicode/bootstrap-datepicker" : "v1.4.0"
    },
    "require-dev": {
        "nette/tester": "~1.4.0"
